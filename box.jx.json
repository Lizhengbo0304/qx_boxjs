    },
    {
      "id": "Zhuzher",
      "name": "住这儿助手",
      "keys": [
        "zhuzher_access_token",
        "zhuzher_refresh_token",
        "zhuzher_user_id"
      ],
      "settings": [
        {
          "id": "zhuzher_access_token",
          "name": "住这儿访问令牌",
